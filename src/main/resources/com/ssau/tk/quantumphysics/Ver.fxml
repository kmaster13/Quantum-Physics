<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            styleClass="background" stylesheets="@style.css"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="624.0"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="com.ssau.tk.quantumphysics.ProbabilityDensityController">
    <HBox alignment="CENTER" prefHeight="75.0" prefWidth="1000.0">
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#getToMainWindow" text="Вернуться в меню"/>
    </HBox>
    <!-- Таблица в которую мы будем выводит наш pojo -->
    <TableView fx:id="tableProb" layoutX="12.0" layoutY="62.0" prefHeight="324.0" prefWidth="600.0">
        <columns>
            <!-- Колонки таблицы -->
            <TableColumn prefWidth="300.0" text="Angle" fx:id="angle"/>
            <TableColumn prefWidth="300.0" text="Prob" fx:id="prob"/>

        </columns>
    </TableView>
    <Label layoutX="14.0" layoutY="18.0" text="Плотность вероятности">
        <font>
            <Font name="Verdana" size="24.0"/>
        </font>
    </Label>
</AnchorPane>